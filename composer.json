{
    "name": "fernandopetry/mvc",
    "description": "Manage mvc calls",
    "keywords": ["fernandopetry", "php", "mvc"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Petry\\MVC\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Petry\\MVC\\": "tests",
            "Petry\\Test\\": "tests/test"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
